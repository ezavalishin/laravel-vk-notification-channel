{
    "name": "ezavalishin/vk-chatbot-notification-channel",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/ezavalishin/vkchatbotnotificationchannel",
    "keywords": ["Laravel", "VkChatBotNotificationChannel"],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0",
        "vkcom/vk-php-sdk": "^5.101"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ezavalishin\\VkChatBotNotificationChannel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ezavalishin\\VkChatBotNotificationChannel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ezavalishin\\VkChatBotNotificationChannel\\VkChatBotNotificationChannelServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
